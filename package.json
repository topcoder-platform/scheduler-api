{
  "name": "topcoder-autopilot-schedule-api",
  "version": "1.0.0",
  "description": "Topcoder Autopilot Schedule V5 API",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "init-db": "node scripts/init-db.js",
    "test-data": "node scripts/test-data.js",
    "test": "nyc --reporter=html --reporter=text mocha test/unit/test.js --timeout 20000 --exit",
    "e2e": "nyc --reporter=html --reporter=text mocha test/e2e/test.js --timeout 20000 --exit"
  },
  "author": "TCSCODER",
  "license": "none",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "@hapi/joi": "^15.0.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "config": "^3.1.0",
    "cors": "^2.8.5",
    "dynamoose": "^1.7.3",
    "express": "^4.17.1",
    "get-parameter-names": "^0.3.0",
    "http-status-codes": "^1.3.2",
    "lodash": "^4.17.19",
    "tc-core-library-js": "appirio-tech/tc-core-library-js.git#v2.6.3",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "nyc": {
    "exclude": [
      "scripts/*.js",
      "test/common/*.js",
      "test/unit/*.js",
      "test/e2e/*.js"
    ]
  }
}
